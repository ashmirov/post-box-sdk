{
  "name": "ashmirov/post-box-sdk",
  "description": "Post box API SDK using PSR-18",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Post\\Box\\Sdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Post\\Box\\Sdk\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8",
    "ext-simplexml": "*",
    "php-http/client-common": "^2.3",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/discovery": "^1.12",
    "psr/http-client": "^1.0",
    "psr/http-client-implementation": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/log": "^3.0",
    "symfony/http-client": "^6.3",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "php-http/mock-client": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
